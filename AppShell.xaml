<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="swissbyte.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:swissbyte"
    xmlns:compra="clr-namespace:swissbyte.Boxes.Compra"
    Shell.FlyoutBehavior="Disabled"
    Title="swissbyte">

    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent 
        Title="" 
        ContentTemplate="{DataTemplate compra:CompraPage}" 
        Route="CompraPage" />

</Shell>
